<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gesture-image="http://schemas.meiying.com/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/px_600"
    android:background="@drawable/ic_launcher" >

    <RelativeLayout
        android:id="@+id/parrent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_600" >

        <ImageView
            android:id="@+id/iv_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/ic_launcher" />

        <LinearLayout
            android:id="@+id/ll_lr_parrent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:visibility="gone" >

            <LinearLayout
                android:id="@+id/ll_left_parrent"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                android:orientation="vertical" >

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_left_up"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_left_up"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_left_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_left_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="00000"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_right_parrent"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                android:orientation="vertical" >

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_right_up"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_right_up"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_right_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_right_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="0000"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_ub_parrent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="@dimen/px_6"
            android:visibility="visible" >

            <LinearLayout
                android:id="@+id/ll_up_parrent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal" >

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_up_left"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_up_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_up_right"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_up_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="00000"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_bottom_parrent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal" >

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_bottom_left"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_bottom_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />

                <com.dd.meiying.wiget.gesturimgview.GestureImageView
                    android:id="@+id/iv_bottom_right"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bg_hecheng"
                    gesture-image:max-scale="10.0"
                    gesture-image:min-scale="0.75" />

                <TextView
                    android:id="@+id/tv_bottom_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="0000"
                    android:textColor="@color/black_65"
                    android:textSize="@dimen/px_32"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>